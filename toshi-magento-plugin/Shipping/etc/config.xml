<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <carriers>
            <toshi>
                <active>1</active>
                <price>0</price>
                <environment>sandbox</environment>
                <model>Toshi\Shipping\Model\Carrier\Shipping</model>
                <name>Free</name>
                <title>Try Before you Buy with TOSHI</title>
                <specificerrmsg>This shipping method is not available. To use this shipping method, please contact us.</specificerrmsg>
                <sort_order>-1</sort_order>
                <toshi_min_basket_amount>350</toshi_min_basket_amount>
                <size_attribute>size</size_attribute>
                <color_attribute>color</color_attribute>
                <timeout>3600</timeout>
                <deferred_days>1</deferred_days>
            </toshi>
        </carriers>
    </default>
</config>
